<?xml version='1.0' encoding='utf-8'?>
<widget id="com.davc.expenses" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Scan">
        <param name="android-package" value="com.neutrinos.plugin.Scan" />
    </feature>
    <feature name="Textocr">
        <param name="android-package" value="com.neutrinos.ocrplugin.Textocr" />
    </feature>
    <name>Receipt Tracker</name>
    <description>
        A app to decode text from a receipt
    </description>
    <author email="2davecollins@gmail.com" href="http://cordova.io">
       Dave Collins
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
